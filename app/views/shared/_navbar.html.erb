<div class="navbar navbar-expand-sm navbar-light navbar-lewagon">
  <%= link_to root_path,  class: "navbar-brand" do %>
    <%= image_tag "https://www.designevo.com/res/templates/thumb_small/retro-black-scooter-head.png" %>
    <% end %>

  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>


  <div class="collapse navbar-collapse" id="navbarSupportedContent">
    <ul class="navbar-nav mr-auto">
      <% if user_signed_in? %>
        <li class="nav-item active">
          <%#= link_to "List Your Scooter", new_scooter_path, class: "nav-link" %>
        </li>
        <li class="nav-item">
          <%#= link_to "Learn More", "#", class: "nav-link" %>
        </li>
        <li class="nav-item dropdown">
          <%= image_tag "data:image/png;base64,", class: "avatar dropdown-toggle", id: "navbarDropdown", data: { toggle: "dropdown" }, 'aria-haspopup': true, 'aria-expanded': false %>
          <div class="dropdown-menu dropdown-menu-right" aria-labelledby="navbarDropdown">
            <%= link_to "My Profile", pages_user_page_path, class: "dropdown-item" %>
            <%= link_to "My Reservations", pages_user_page2_path, class: "dropdown-item" %>
            <%= link_to "My Listings", pages_user_page3_path, class: "dropdown-item" %>
            <%= link_to "My Request", pages_user_page4_path, class: "dropdown-item" %>
            <%= link_to "Log out", destroy_user_session_path, method: :delete, class: "dropdown-item" %>
          </div>
        </li>
      <% else %>
        <li class="nav-item">
          <%= link_to "Login", new_user_session_path, class: "nav-link" %>
        </li>
      <% end %>
    </ul>
  </div>
</div>
